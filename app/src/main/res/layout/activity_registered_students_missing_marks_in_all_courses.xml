<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout
xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:fillViewport="true"
tools:context=".home.admin.StudentsPerformanceBar.RegisteredStudentsMissingMarksInAllCourses">
<com.google.android.material.appbar.AppBarLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/RegisteredStudentsMissingMarksInAllCourses_toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="?attr/colorPrimary"
        app:title="Missing Marks In All Courses List"
        app:titleTextColor="@android:color/white"
        app:titleTextAppearance="@style/ToolbarTitle"
        android:gravity="center"
        />
</com.google.android.material.appbar.AppBarLayout>
    <FrameLayout
        android:id="@+id/RegisteredStudentsMissingMarksInAllCourses"
        android:layout_width="match_parent"
        android:layout_marginTop="60dp"
        android:layout_height="match_parent">
        <ListView
            android:id="@+id/registeredStudentsMissingMarksInAllCoursesListView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="1dp"
            android:layout_marginTop="50dp"
            android:layout_marginEnd="1dp"
            android:padding="2dp"/>
    </FrameLayout>
</androidx.coordinatorlayout.widget.CoordinatorLayout>

